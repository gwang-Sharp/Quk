
@{
    ViewBag.Title = "周人天汇总";
    Layout = "~/Views/Shared/_LayoutPhone.cshtml";
    var detailsData = ViewBag.details;
}
@*<link rel="stylesheet" href="https://cdn.bootcss.com/weui/1.1.3/style/weui.min.css">
    <link rel="stylesheet" href="https://cdn.bootcss.com/jquery-weui/1.2.1/css/jquery-weui.min.css">*@
<style type="text/css">
    .box11 {
        display: -webkit-flex; /* Safari */
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        align-items: center
    }

    .Mainbox {
        display: -webkit-flex; /* Safari */
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-items: center
    }

    .Mainbox_Project {
        display: -webkit-flex; /* Safari */
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-items: center;
        /*border-bottom: 0.5px dashed #C0C0C0*/
    }

    .Box_Process {
        width: 100%;
        display: flex;
        justify-content: flex-start;
        align-items: center;
        padding-left: 10px;
        font-size: 12px;
        padding-top: 5px;
        padding-bottom: 5px;
    }

    .Mainbox_Process {
        display: -webkit-flex; /* Safari */
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-items: center
    }

    .Buttonbox {
        display: -webkit-flex; /* Safari */
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-end;
        align-items: flex-start
    }

    .listborder {
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
        /* border-top: 0.5px solid #C0C0C0; */
        /* border-bottom: 0.5px solid #C0C0C0; */
        /* border-left: 0.5px solid #C0C0C0; */
        /* border-right: 0.5px solid #C0C0C0; */
        margin-bottom: 10px;
        margin-top: 10px;
        margin-right: 10px;
        margin-left: 10px;
        padding: 3px 1px 3px 1px;
        background-color: #ffffff;
        box-shadow: 2px 2px 1px 1px rgba(21, 1, 1, 0.1);
    }

    .tag_status1 {
        padding: 2px 4px;
        background-color: #F56C6C;
        color: #fff;
    }

    .tag_status2 {
        padding: 2px 4px;
        background-color: #E2DA3A;
        color: #fff;
    }

    .tag_status3 {
        padding: 2px 4px;
        background-color: #67C23A;
        color: #fff;
    }

    .ProjectTitle {
        padding-top: 5px;
        padding-left: 10px;
        padding-bottom: 5px;
        font-size: 14px;
    }

    .ProjectContent {
        line-height: 20px;
        padding-left: 10px;
        font-size: 12px;
        padding-top: 5px;
        padding-bottom: 5px;
    }

    .Button {
        line-height: 0.8;
        white-space: nowrap;
        cursor: pointer;
        background: #fff;
        border: 1px solid #dcdfe6;
        color: #606266;
        -webkit-appearance: none;
        text-align: center;
        box-sizing: border-box;
        outline: none;
        margin: 0;
        transition: .1s;
        font-weight: 500;
        -moz-user-select: none;
        -webkit-user-select: none;
        -ms-user-select: none;
        padding: 12px 20px;
        font-size: 14px;
        border-radius: 4px;
        color: #fff;
        background-color: #409eff;
        border-color: #409eff;
        margin-left: 5px;
        margin-right: 5px;
    }

    .mt-progress-runway {
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
    }

    .weui-search-bar {
        box-shadow: 1px 1px 1px 1px rgba(21, 1, 1, 0.1);
        padding: 0px;
    }

    .searchhide {
        display: none
    }

    .searchshow {
        display: block
    }

    * {
        margin: 0;
        padding: 0;
    }

    html, body {
        width: 100%;
        height: 100%;
    }
</style>
<style>
    .el-card__body {
        margin-bottom: 20px;
    }
</style>
<div id="allreport" style="background-color:#F5F5F8;padding-top:10px;height:100%">
    <div class="container" style="background-color:#F5F5F8;margin:10px 10px;">
        @*<div class="weui-search-bar listborder" id="searchBar">*@
        @*<form class="weui-search-bar__form" @@submit.prevent>
                <div class="weui-search-bar__box">
                    <i class="weui-icon-search"></i>
                    <input v-model="Person" @@keyup.13="onSubmit" class="weui-search-bar__input" id="searchInput" placeholder="搜索">
                    <a href="javascript:" v-on:click="SearchReset" class="weui-icon-clear" id="searchClear"></a>
                </div>
                <label class="weui-search-bar__label" id="searchText" v-on:click="Search" v-bind:class="SearchBar.Labledisplay">
                    <i class="weui-icon-search"></i>
                    <span>搜索</span>
                </label>
            </form>*@
        @*<label  v-on:click="SearchReset" style="padding-right: 10px;padding-left: 10px;background-color: #e5e5e5;border-radius:5px;line-height: 2" class="weui-search-bar__cancel-btn;"  v-bind:class="SearchBar.adisplay" id="searchCancel">取消</label>*@
        @*</div>*@
        <el-tag type="info" v-html="Person"></el-tag>
        <el-divider></el-divider>
        <el-form>
            <el-form-item>
                <el-col :span="8" style="margin-top:5px">
                    <label>
                        <font color="black" style="font-family:微软雅黑;margin-left:30px;font-size:18px;text-align:right" v-html="'日期:'"></font>
                    </label>
                </el-col>
                <el-col :span="16" style="margin-top:5px">
                    <el-form-item>
                        <el-date-picker v-model="Week"
                                        type="week"
                                        format="yyyy 第 WW 周"
                                        placeholder="选择周"
                                        :clearable="false"
                                        v-on:change="onSubmit">
                        </el-date-picker>
                    </el-form-item>
                </el-col>
            </el-form-item>
        </el-form>
        <div v-show="allProjectDetails.Array">
            <label><font color="black" style="font-family:微软雅黑;margin-left:30px;font-size:18px" v-html="'项目名称:'"></font> </label>
            <label><font color="black" style="font-family:微软雅黑;margin-left:40px;font-size:20px;font-weight:bold" v-html="allProjectDetails.ProjectName"></font></label>
            <el-divider></el-divider>
        </div>
        <template>
            <div v-for="item in allProjectDetails.Array" v-cloak style="margin-top:10px;">
                <el-card class="box-card" shadow="always">
                    <div class="text item" style="float:left;margin-left:50px" v-html="TimestampToDate(item[0].CreateTime)"></div>
                    <div class="text item" style="float:right;margin-right:50px;" v-html="item[0].WorkingTime+'/天'"></div>
                </el-card>
            </div>
        </template>
    </div>
    <div style="display:none;">
        <input value="@detailsData" id="DataStore" hidden="hidden" />
    </div>
</div>
@Scripts.Render("~/bundles/AllReportDetails?v="+DateTime.Now.Millisecond)
<!-- body 最后 -->
<script src="~/Scripts/jquery-weui.min.js"></script>


